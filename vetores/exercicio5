def digitar_elementos_vetor(num_vezes):
    vetor = []
    for i in range(num_vezes):
        valor = int(input("Digite um numero: "))
        vetor.append(valor)
    return vetor

def somar_elementos_vetor(vetor):
    soma = 0
    for i in range(len(vetor)):
        soma += vetor[i]
    return soma

#vetor = [2, 3, 5, 7, 8, 9]

num_vezes = int(input("Digite a quantidade de elementos no vetor: "))
vetor = digitar_elementos_vetor(num_vezes)
soma = somar_elementos_vetor(vetor)
print(f"Soma = {soma}")