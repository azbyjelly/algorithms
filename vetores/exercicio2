vetor = [1, 5, 7, 2, 4, 6, 3, 8, 10]
maior = vetor [0]
for valor in vetor:
    if valor > maior:
        maior = valor

print("Maior valor: ", maior)


#com def

def receber_valores(qtd_vezes):
    vetor = []
    for j in range(qtd_vezes):
        valor = int(input("digite um valor: "))
        vetor.append(valor)
    return vetor


def encontrar_maior():
    maior = vetor[0]
    for i in range(len(vetor)):
        if vetor[i] > maior:
            maior = vetor[i]
    return maior

qtd_vezes = int(input("digite quantas vezes: "))
vetor = receber_valores(qtd_vezes)
maior = encontrar_maior()
print(maior)
