count_dentro = 0
count_fora = 0

for i in range(10):
    valor = int(input("Digite um valor: "))
    if 10 <= valor <= 20:
        count_dentro += 1
    else:
        count_fora += 1

print("Quantidade de valores dentro do intervalo [10, 20]:", count_dentro)
print("Quantidade de valores fora do intervalo [10, 20]:", count_fora)
